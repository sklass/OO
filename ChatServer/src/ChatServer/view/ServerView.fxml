<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ChatServer.controller.ServerController">
   <children>
      <TabPane layoutY="1.0" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Main">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="StartBtn" layoutX="19.0" layoutY="61.0" mnemonicParsing="false" text="Start" />
                        <TextField fx:id="PortTextField" layoutX="40.0" layoutY="27.0" prefHeight="25.0" prefWidth="54.0" text="8080" />
                        <Button fx:id="StopBtn" layoutX="62.0" layoutY="61.0" mnemonicParsing="false" text="Stop" />
                        <Label layoutX="14.0" layoutY="31.0" text="Port:" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Log">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea fx:id="LogTextArea" layoutX="14.0" layoutY="35.0" prefHeight="301.0" prefWidth="422.0" />
                        <Label layoutX="14.0" layoutY="14.0" text="Label" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
